defmodule <%= @module %> do
  <%= if @module_doc? do %>
  <%= if @comment != "" do %>
  @moduledoc """
<%= Protobuf.Protoc.Generator.Util.pad_comment(@comment, 2) %>
  """
  <% end %>
  <% else %>
  @moduledoc false
  <% end %>

  use Protobuf, <%= @use_options %>

  <% if @extends == [], do: raise("Fuck! #{@module}") %>

  <%= for ext <- @extends do %>
  extend <%= ext %>
  <% end %>
end
